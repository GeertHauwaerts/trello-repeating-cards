{
  "name": "geerthauwaerts/trello-repeating-cards",
  "description": "Trello - Add Repeating Cards",
  "keywords": [
    "trello",
    "repeat"
  ],
  "homepage": "https://github.com/geerthauwaerts/trello-repeating-cards",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Geert Hauwaerts",
      "email": "geert@hauwaerts.be"
    }
  ],
  "require": {
    "php": ">=7.0",
    "cdaguerre/php-trello-api": "^0.9.3",
    "nesbot/carbon": "^2.34",
    "vlucas/phpdotenv": "^4.1"
  },
  "config": {
    "process-timeout": 0,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "scripts": {
    "phpcs": "phpcs --standard=phpcs.xml --extensions=php app/"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5"
  }
}
